<% if !user_logged_in? %>
 <video loop="loop" preload="auto"  id='videoplayer' style='width:100%' autoplay muted>
   <source src="assets/splash.mov" type="video/mp4">
  </video>
<% elsif current_user.is_teacher? %>
  <%= render partial: '/users/teachers/dashboard' %>
<% else %>
  <%= render partial: '/users/students/student_dashboard' %>
<% end %>
